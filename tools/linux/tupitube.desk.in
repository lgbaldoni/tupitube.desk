#!/bin/bash

export TUPITUBE_HOME="@TUPITUBE_HOME@"
export TUPITUBE_SHARE="@TUPITUBE_SHARE@"
export TUPITUBE_LIB="@TUPITUBE_LIB@"
export TUPITUBE_PLUGIN="@TUPITUBE_PLUGIN@"
export TUPITUBE_BIN="@TUPITUBE_BIN@"

export LD_LIBRARY_PATH="./lib/libav:./lib/quazip:./lib/qt:${TUPITUBE_LIB}:${TUPITUBE_PLUGIN}:$LD_LIBRARY_PATH"

exec ${TUPITUBE_BIN}/tupitube.bin "$@"
